    "@tabler/icons": "^1.112.0",
    "easymde": "^2.18.0",
    "electron-updater": "^5.3.0",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "normalize.css": "^8.0.1",
    "react-simplemde-editor": "^5.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@vitejs/plugin-react": "^2.2.0",
    "electron": "^21.3.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.14",
    "eslint": "^8.28.0",
